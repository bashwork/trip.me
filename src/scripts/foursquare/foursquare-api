            {                                                                                                                                       
                name:'Newbie',                                                                                                                      
                icon:'http://foursquare.com/img/badge/newbie_on.png',                                                                               
                description:'Congrats on your first check-in!'                                                                                      
            }                                                                                                                                       
                                                                                                                                                    
Response for 'self' authenticated (truncated):                                                                                                      
                                                                                                                                                    
{                                                                                                                                                   
